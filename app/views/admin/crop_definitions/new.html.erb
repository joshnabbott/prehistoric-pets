<h1>New crop definition</h1>

<% form_for([:admin, @crop_definition]) do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <p>
      <%= f.submit "Create", :class => 'submit' %> - or - <%= link_to 'Cancel', admin_crop_definitions_path %>
    </p>
  </fieldset>
<% end %>